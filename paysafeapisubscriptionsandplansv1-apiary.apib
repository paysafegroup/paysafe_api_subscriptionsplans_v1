        "locale": "en_US",
        "firstName": "John",
        "middleName": "James",
        "lastName": "Smith",
        "dateOfBirth": {
            "year": 1990,
            "month": 12,
            "day": 29
        },
        "gender": "M",
        "paymentToken": "CNjEzMTM4NDg1"
    }
